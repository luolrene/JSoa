import axios from '@/utils/axios'

// 借款管理-查询（分页）
export function getFinanceLoanQueryPageData(params) {
  return new Promise((resolve, reject) => {
    axios.post(process.env.JS_Server + '/FinanceLoan/queryPageData', params).then(res => {
      resolve(res.data)
    }).catch(err => {
      reject(err)
    })
  })
}
