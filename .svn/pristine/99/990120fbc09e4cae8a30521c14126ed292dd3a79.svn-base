import axios from '@/utils/axios'

// 报销管理-查询（分页）
export function getReimbursementQueryPageData(params) {
  return new Promise((resolve, reject) => {
    axios.post(process.env.JS_Server + '/Reimbursement/queryPageData', params).then(res => {
      resolve(res.data)
    }).catch(err => {
      reject(err)
    })
  })
}
