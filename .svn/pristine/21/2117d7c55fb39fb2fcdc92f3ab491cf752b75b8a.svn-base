import axios from '@/utils/axios'
/* 结构树 */
export function getAssetsTypeGetChildren(params) {
  return new Promise((resolve, reject) => {
    axios.post(process.env.JS_Server + '/AssetsType/getChildren', params).then(res => {
      resolve(res.data)
    }).catch(err => {
      reject(err)
    })
  })
}

